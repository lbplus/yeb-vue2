r>\r\n                  <td>奖金</td>\r\n                  <td>{{ scope.row.s.bonus }}</td>\r\n                </tr>\r\n                <tr>\r\n                  <td>养老金比率</td>\r\n                  <td>{{ scope.row.s.pensionPer }}</td>\r\n                </tr>\r\n                <tr>\r\n                  <td>养老金基数</td>\r\n                  <td>{{ scope.row.s.pensionBase }}</td>\r\n                </tr>\r\n                <tr>\r\n                  <td>医疗保险比率</td>\r\n                  <td>{{ scope.row.s.medicalPer }}</td>\r\n                </tr>\r\n                <tr>\r\n                  <td>医疗保险金基数</td>\r\n                  <td>{{ scope.row.s.medicalBase }}</td>\r\n                </tr>\r\n                <tr>\r\n                  <td>公积金比率</td>\r\n                  <td>{{ scope.row.s.accumulationFundPer }}</td>\r\n                </tr>\r\n                <tr>\r\n                  <td>公积金基数</td>\r\n                  <td>{{ scope.row.s.accumulationFundBase }}</td>\r\n                </tr>\r\n              </table>\r\n            </div>\r\n            <el-tag type=\"success\" v-if=\"scope.row.s.name\">{{\r\n              scope.row.s.name\r\n            }}</el-tag>\r\n            <el-tag v-else type=\"primary\">暂未设置</el-tag>\r\n          </el-tooltip>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-popover\r\n            placement=\"left\"\r\n            title=\"编辑工资账套\"\r\n            @show=\"showPop(scope.row.s)\"\r\n            @hide=\"hide(scope.row)\"\r\n            width=\"200\"\r\n            trigger=\"click\"\r\n          >\r\n            <div>\r\n              <el-select v-model=\"currentSalary\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"item in salaries\"\r\n                  :key=\"item.id\"\r\n                  :label=\"item.name\"\r\n                  :value=\"item.id\"\r\n                >\r\n                </el-option>\r\n              </el-select>\r\n            </div>\r\n            <el-button slot=\"reference\" type=\"danger\"> 修改工资账套</el-button>\r\n          </el-popover>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"display: flex; justify-content: flex-end; margin-top: 10px\">\r\n      <el-pagination\r\n        background\r\n        @current-change=\"currentChange\"\r\n        @size-change=\"sizeChange\"\r\n        layout=\"prev, pager, next, jumper, ->, total, slot\"\r\n        :total=\"total\"\r\n      ></el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"SalSobCfg\",\r\n  data() {\r\n    return {\r\n      currentSalary: null,\r\n      salaries: [],\r\n      emps: [],\r\n      currentPage: 1,\r\n      size: 10,\r\n      total: 0,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.initEmps();\r\n    this.initSalaries();\r\n  },\r\n  methods: {\r\n    hide(data) {\r\n      if (this.currentSalary && this.currentSalary != data.s.id) {\r\n        this.putRequest(\r\n          \"/salary-adjust/update?eid=\" + data.id + \"&eSid=\" + this.currentSalary\r\n        ).then((resp) => {\r\n          if (resp) {\r\n            this.intEmps();\r\n          }\r\n        });\r\n      }\r\n    },\r\n    showPop(data) {\r\n      if (data) {\r\n        this.currentSalary = data.id;\r\n      } else {\r\n        this.currentSalary = null;\r\n      }\r\n    },\r\n    initSalaries() {\r\n      this.getRequest(\"/salary-adjust/get-All\").then((resp) => {\r\n        if (resp) {\r\n          this.salaries = resp;\r\n        }\r\n      });\r\n    },\r\n    currentChange(page) {\r\n      this.currentPage = page;\r\n      this.initEmps();\r\n    },\r\n    sizeChange(size) {\r\n      this.size = size;\r\n      this.initEmps();\r\n    },\r\n    initEmps() {\r\n      this.getRequest(\r\n        \"/salary-adjust/list?currentPage=\" +\r\n          this.currentPage +\r\n          \"&size=\" +\r\n          this.size\r\n      ).then((resp) => {\r\n        if (resp) {\r\n          this.emps = resp.date;\r\n          this.total = resp.total;\r\n          console.log(resp);\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/views/sal"}]}