AA,IAAA,EAAA,EAAA;AACA,UAAA,IAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;;AACA,cAAA,IAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,YAAA,CAAA,CAAA,QAAA,GAAA,KAAA;AACA;;AACA;AACA,SANA,MAMA;AACA,eAAA,iBAAA,CAAA,CAAA,EAAA,CAAA,CAAA,QAAA,EAAA,EAAA;AACA;AACA;AACA,KAdA;AAeA,IAAA,SAfA,qBAeA,IAfA,EAeA;AAAA;;AACA,UAAA,IAAA,CAAA,QAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,UAAA;AACA,OAFA,MAEA;AACA,aAAA,QAAA,CACA,YAAA,IAAA,CAAA,IAAA,GAAA,YADA,EAEA,IAFA,EAGA;AACA,UAAA,iBAAA,EAAA,IADA;AAEA,UAAA,gBAAA,EAAA,IAFA;AAGA,UAAA,IAAA,EAAA;AAHA,SAHA,EASA,IATA,CASA,YAAA;AACA,UAAA,KAAA,CAAA,aAAA,CACA,wCAAA,IAAA,CAAA,EADA,EAEA,IAFA,CAEA,UAAA,IAAA,EAAA;AACA,gBAAA,IAAA,EAAA;AACA,cAAA,KAAA,CAAA,iBAAA,CAAA,IAAA,EAAA,KAAA,CAAA,IAAA,EAAA,IAAA,CAAA,EAAA;AACA;AACA,WANA;AAOA,SAjBA,EAkBA,KAlBA,CAkBA,YAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,MADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA,SAvBA;AAwBA;AACA,KA5CA;AA8CA,IAAA,QA9CA,sBA8CA;AAAA;;AACA,WAAA,WAAA,CAAA,8BAAA,EAAA,KAAA,GAAA,EAAA,IAAA,CACA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA;AACA,OANA;AAQA,KAvDA;AAwDA,IAAA,UAxDA,sBAwDA,IAxDA,EAwDA;AACA,MAAA,OAAA,CAAA,GAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,KAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,GAAA,CAAA,QAAA,GAAA,IAAA,CAAA,EAAA;AACA,KA7DA;AA8DA,IAAA,QA9DA,sBA8DA;AAAA;;AACA,WAAA,UAAA,CAAA,+BAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA;AACA,OAJA;AAKA,KApEA;AAqEA,IAAA,UArEA,sBAqEA,KArEA,EAqEA,IArEA,EAqEA;AACA,UAAA,CAAA,KAAA,EAAA,OAAA,IAAA;AACA,aAAA,IAAA,CAAA,IAAA,CAAA,OAAA,CAAA,KAAA,MAAA,CAAA,CAAA;AACA;AAxEA;AA1BA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"width: 500px\">\r\n      <el-input\r\n        placeholder=\"输入需要搜索的内容\"\r\n        prefix-icon=\"el-icon-search\"\r\n        v-model=\"filterText\"\r\n      >\r\n      </el-input>\r\n\r\n      <el-tree\r\n        style=\"margin-top: 20px\"\r\n        :data=\"deps\"\r\n        :props=\"defaultProps\"\r\n        :filter-node-method=\"filterNode\"\r\n        :expand-on-click-node=\"false\"\r\n        ref=\"tree\"\r\n      >\r\n        <span\r\n          slot-scope=\"{ node, data }\"\r\n          style=\"display: flex; justify-content: space-between; width: 100%\"\r\n        >\r\n          <span>{{ node.label }}</span>\r\n          <span>\r\n            <el-button\r\n              style=\"padding: 2px\"\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              @click=\"() => showAddDep(data)\"\r\n            >\r\n              添加部门\r\n            </el-button>\r\n            <el-button\r\n              style=\"padding: 2px\"\r\n              type=\"danger\"\r\n              size=\"mini\"\r\n              @click=\"() => deleteDep(data)\"\r\n            >\r\n              删除部门\r\n            </el-button>\r\n          </span>\r\n        </span>\r\n      </el-tree>\r\n      <el-dialog title=\"添加部门\" :visible.sync=\"dialogVisible\" width=\"30%\">\r\n        <div>\r\n          <table>\r\n            <tr>\r\n              <td><el-tag>上级部门</el-tag></td>\r\n              <td>{{ pname }}</td>\r\n            </tr>\r\n            <tr>\r\n              <td><el-tag>部门名称</el-tag></td>\r\n              <td>\r\n                <el-input\r\n                  v-model=\"dep.name\"\r\n                  placeholder=\"请输入部门名称\"\r\n                ></el-input>\r\n              </td>\r\n            </tr>\r\n          </table>\r\n        </div>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"doAddDep\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"DepMana\",\r\n  data() {\r\n    return {\r\n      filterText: \"\",\r\n      deps: [],\r\n      defaultProps: {\r\n        children: \"children\",\r\n        label: \"name\",\r\n      },\r\n      dialogVisible: false,\r\n      dep: {\r\n        name: \"\",\r\n        parentId: \"\",\r\n      },\r\n      pname: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.initDeps();\r\n  },\r\n  watch: {\r\n    filterText(val) {\r\n      this.$refs.tree.filter(val);\r\n    },\r\n  },\r\n  methods: {\r\n    removeDepFromDeps(p, deps, id) {\r\n      for (let i = 0; i < deps.length; i++) {\r\n        let d = deps[i];\r\n        if (d.id == id) {\r\n          deps.splice(i, 1);\r\n          if (deps.length == 0) {\r\n            p.isParent = false;\r\n          }\r\n          return;\r\n        } else {\r\n          this.removeDepFromDeps(d, d.children, id);\r\n        }\r\n      }\r\n    },\r\n    deleteDep(data) {\r\n      if (data.isParent) {\r\n        this.$message.error(\"父部门删除失败!\");\r\n      } else {\r\n        this.$confirm(\r\n          \"此操作将删除[\" + data.name + \"]部门, 是否继续?\",\r\n          \"提示\",\r\n          {\r\n            confirmButtonText: \"确定\",\r\n            cancelButtonText: \"取消\",\r\n            type: \"warning\",\r\n          }\r\n        )\r\n          .then(() => {\r\n            this.deleteRequest(\r\n              \"/system/basic/department/delete?id=\" + data.id\r\n            ).then((resp) => {\r\n              if (resp) {\r\n                this.removeDepFromDeps(null, this.deps, data.id);\r\n              }\r\n            });\r\n          })\r\n          .catch(() => {\r\n            this.$message({\r\n              type: \"info\",\r\n              message: \"已取消删除\",\r\n            });\r\n          });\r\n      }\r\n    },\r\n\r\n    doAddDep() {\r\n      this.postRequest(\"/system/basic/department/add\", this.dep).then(\r\n        (resp) => {\r\n          if (resp) {\r\n            this.initDeps();\r\n            this.dialogVisible = false;\r\n          }\r\n        }\r\n      );\r\n    },\r\n    showAddDep(data) {\r\n      console.log();\r\n      this.dialogVisible = true;\r\n      this.pname = data.name;\r\n      this.dep.parentId = data.id;\r\n    },\r\n    initDeps() {\r\n      this.getRequest(\"/system/basic/department/list\").then((resp) => {\r\n        if (resp) {\r\n          this.deps = resp;\r\n        }\r\n      });\r\n    },\r\n    filterNode(value, data) {\r\n      if (!value) return true;\r\n      return data.name.indexOf(value) !== -1;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/components/sys/basic"}]}