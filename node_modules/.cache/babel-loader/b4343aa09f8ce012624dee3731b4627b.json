AA,uBAAA,GAAA,EAAA,IAAA,CACA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,aAAA;AACA;AACA,SALA;AAOA,OApBA,EAoBA,KApBA,CAoBA,YAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAzBA;AA0BA,KA7BA;AA8BA,IAAA,qBA9BA,iCA8BA,GA9BA,EA8BA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA,KAhCA;AAiCA;AACA,IAAA,UAlCA,sBAkCA,KAlCA,EAkCA,IAlCA,EAkCA;AACA,MAAA,MAAA,CAAA,MAAA,CAAA,KAAA,SAAA,EAAA,IAAA,EADA,CACA;;AACA,WAAA,SAAA,CAAA,UAAA,GAAA,EAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,KAtCA;AAuCA;AACA,IAAA,QAxCA,sBAwCA;AAAA;;AACA,WAAA,UAAA,CAAA,oBAAA,EAAA,KAAA,SAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,aAAA;;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA;AACA,OALA;AAOA,KAhDA;AAiDA;AACA,IAAA,WAlDA,yBAkDA;AAAA;;AACA,UAAA,KAAA,GAAA,CAAA,IAAA,EAAA;AACA,aAAA,WAAA,CAAA,oBAAA,EAAA,KAAA,GAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,aAAA;;AACA,YAAA,MAAA,CAAA,GAAA,CAAA,IAAA,GAAA,EAAA;AACA;AACA,SALA;AAOA,OARA,MAQA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,WAAA;AACA;AACA,KA9DA;AA+DA;AACA,IAAA,YAhEA,wBAgEA,KAhEA,EAgEA,IAhEA,EAgEA;AAAA;;AACA,WAAA,QAAA,CAAA,QAAA,IAAA,CAAA,IAAA,GAAA,YAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,aAAA,CAAA,uBAAA,IAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,aAAA;AACA;AACA,SAJA;AAKA,OAXA,EAWA,KAXA,CAWA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAhBA;AAiBA,KAlFA;AAmFA;AACA,IAAA,aApFA,2BAoFA;AAAA;;AACA,WAAA,UAAA,CAAA,oBAAA,EAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA;AACA,OAJA;AAKA;AA1FA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <el-input\r\n          size=\"small\"\r\n          placeholder=\"添加职位\"\r\n          prefix-icon=\"el-icon-plus\"\r\n          v-model=\"pos.name\"\r\n          class=\"addPosInput\"\r\n          @keydown.enter.native=\"addPosition\"\r\n      >\r\n      </el-input>\r\n      <el-button\r\n          size=\"small\"\r\n          icon=\"el-icon-plus\"\r\n          type=\"primary\"\r\n          @click=\"addPosition\"\r\n      >添加\r\n      </el-button\r\n      >\r\n    </div>\r\n    <div>\r\n      <el-table\r\n          size=\"small\"\r\n          stripe\r\n          border\r\n          :data=\"positions\"\r\n          style=\"width: 100%\"\r\n          class=\"posManaMain\"\r\n          @selection-change=\"handleSelectionChange\">\r\n        <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n        <el-table-column prop=\"id\" label=\"编号\" width=\"180\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"职位\" width=\"120\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"createDate\" label=\"创建时间\" width=\"200\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"enabled\" label=\"是否启用\" width=\"120\">\r\n\r\n          <template slot-scope=\"scope\">\r\n            <el-tag type=\"success\" v-if=\"scope.row.enabled\">已启用</el-tag>\r\n            <el-tag type=\"danger\" v-else>未启用</el-tag>\r\n          </template>\r\n\r\n        </el-table-column>\r\n\r\n        <el-table-column label=\"操作\" width=\"150\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button size=\"mini\" @click=\"handleEdit(scope.$index, scope.row)\">编辑</el-button>\r\n            <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"handleDelete(scope.$index, scope.row)\">删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n\r\n      </el-table>\r\n    </div>\r\n    <el-button\r\n        type=\"danger\"\r\n        style=\"margin-top=10px\"\r\n        :disabled=\"this.multipleSelection.length == 0\"\r\n        @click=\"deleteMany\"\r\n    >批量删除\r\n    </el-button\r\n    >\r\n    <el-dialog title=\"编辑职位\" :visible.sync=\"dialogVisible\" width=\"30%\">\r\n      <div>\r\n        <el-tag>职位名称</el-tag>\r\n        <el-input\r\n            v-model=\"updatePos.name\"\r\n            size=\"small\"\r\n            class=\"updataPosInput\"\r\n        ></el-input>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button size=\"small\" @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button size=\"small\" type=\"primary\" @click=\"doupdate\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"PosMana\",\r\n  data() {\r\n    return {\r\n      pos: {\r\n        name: \"\",\r\n      },\r\n      positions: [],\r\n      dialogVisible: false,\r\n      updatePos: {\r\n        name: \"\",\r\n      },\r\n      multipleSelection: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.initPositions();\r\n  },\r\n  methods: {\r\n    //批量删除\r\n    deleteMany() {\r\n      this.$confirm(\r\n          \"删除[\" + this.multipleSelection.length + \"]个职位, 是否继续?\",\r\n          \"提示\",\r\n          {\r\n            confirmButtonText: \"确定\",\r\n            cancelButtonText: \"取消\",\r\n            type: \"warning\",\r\n          }\r\n      ).then(() => {\r\n        let ids = \"?\";\r\n        this.multipleSelection.forEach((item) => {\r\n          ids += \"ids=\" + item.id + \"&\";\r\n        });\r\n        this.deleteRequest(\"/system/basic/pos/\" + ids).then(\r\n            (res) => {\r\n              if (res) {\r\n                this.initPositions();\r\n              }\r\n            }\r\n        );\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: \"info\",\r\n          message: \"已取消删除\",\r\n        });\r\n      });\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = val;\r\n    },\r\n    //点击编辑框\r\n    handleEdit(index, data) {\r\n      Object.assign(this.updatePos, data);//这里是拷贝，不是直接赋值\r\n      this.updatePos.createDate = '';\r\n      this.dialogVisible = true;\r\n    },\r\n    //更新\r\n    doupdate() {\r\n      this.putRequest(\"/system/basic/pos/\", this.updatePos).then((resp) => {\r\n            if (resp) {\r\n              this.initPositions();\r\n              this.dialogVisible = false;\r\n            }\r\n          }\r\n      );\r\n    },\r\n    //添加\r\n    addPosition() {\r\n      if (this.pos.name) {\r\n        this.postRequest(\"/system/basic/pos/\", this.pos).then((resp) => {\r\n              if (resp) {\r\n                this.initPositions();\r\n                this.pos.name = \"\";\r\n              }\r\n            }\r\n        );\r\n      } else {\r\n        this.$message.error(\"职位名称不能为空!\");\r\n      }\r\n    },\r\n    //删除\r\n    handleDelete(index, data) {\r\n      this.$confirm(\"删除[\" + data.name + \"]职位, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      }).then(() => {\r\n        console.log(data);\r\n        this.deleteRequest(\"/system/basic/pos/\" + data.id).then((res) => {\r\n          if (res) {\r\n            this.initPositions();\r\n          }\r\n        });\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: \"info\",\r\n          message: \"已取消删除\",\r\n        });\r\n      });\r\n    },\r\n    //获取所有\r\n    initPositions() {\r\n      this.getRequest(\"/system/basic/pos/\").then((resp) => {\r\n        if (resp) {\r\n          this.positions = resp;\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.addPosInput {\r\n  width: 300px;\r\n  margin-right: 10px;\r\n}\r\n\r\n.posManaMain {\r\n  margin-top: 10px;\r\n}\r\n\r\n.updataPosInput {\r\n  width: 180px;\r\n  margin-left: 10px;\r\n}\r\n</style>"],"sourceRoot":"src/components/sys/basic"}]}